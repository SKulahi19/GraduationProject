<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.kulahi.buyurkenbuyut.Takvim">

    <Button
        android:layout_width="39dp"
        android:layout_height="42dp"
        android:id="@+id/gunBir"
        android:onClick="onClickTakvim"
        android:background="@drawable/bir"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.345"
        app:layout_constraintVertical_bias="0.381" />

    <Button
        android:layout_width="39dp"
        android:layout_height="42dp"
        android:id="@+id/gunIki"
        android:onClick="onClickTakvim"
        android:background="@drawable/iki"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.381"
        app:layout_constraintHorizontal_bias="0.501" />

    <Button
        android:layout_width="39dp"
        android:layout_height="42dp"
        android:id="@+id/gunUc"
        android:onClick="onClickTakvim"
        android:background="@drawable/uc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.644"
        app:layout_constraintVertical_bias="0.381" />

    <Button
        android:layout_width="39dp"
        android:layout_height="42dp"
        android:id="@+id/gunDort"
        android:onClick="onClickTakvim"
        android:background="@drawable/dort"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.794"
        app:layout_constraintVertical_bias="0.381" />



</android.support.constraint.ConstraintLayout>
